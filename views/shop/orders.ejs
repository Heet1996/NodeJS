<%-include('../includes/head.ejs')  %>

    <%-include('../includes/navigation.ejs')  %>
        <main>
            <% if(orders.length<=0) { %>
            <h1>Nothings here !</h1>
            <%  }else {   %>
                <ul>
                    <% orders.forEach( order => { %>
                        <li>
                            <h1># <%=order.id %></h1>
                            <ul>
                                <% order.products.forEach( product => { %>
                                    <li><%=product.title %>( <%=product.orderItem.quantityItem %> )</li>
                                <% })  %>    
                            </ul>
                        </li>
                    <% }) %>    
                </ul>
            <% } %>    
        </main>
    <%-include('../includes/footer.ejs')  %>